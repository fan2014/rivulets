
<div class="row">
	<aside class="span4">
		<section>
			<h1>
				<%= image_tag @user.image_url(:thumb).to_s %> <br />
				<%= @user.name %>
			</h1>
		</section>
	</aside>
</div>

<%= form_for @user, :html => { :multipart => true } do |f| %>
  <%= f.text_field :name %> <br />
	<%= f.file_field :image %> <br />
  <%= f.submit %>
<% end %>



<%= @test %>